    "def maxfunction(nums):\n",
    "    l=[]\n",
    "    for i in nums:\n",
    "        count=0\n",
    "        for j in nums:\n",
    "            if i > j:\n",
    "                count=count+1\n",
    "        l.append(count)\n",
    "                \n",
    "                \n",
    "    return l"
   ]
